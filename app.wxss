/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.kongbao-head{
  height: 90rpx;
  width: 100%;
  position: fixed;
  top: 0;
  padding-top:40rpx;
  background-color: #fff;
  z-index: 1000;
  text-align:center;
  line-height:110rpx;
  font-size: 30rpx;
}

.pape{
  padding-top: 136rpx;
  z-index: 1;
}
.left-icon{
	width:50rpx;
	height:50rpx;
	position:absolute;
	top:50%;
	margin-top:4rpx;
	left:30rpx;
	background-image: url(data:image/png;base64,);
	background-size:100% 100%;
}
.left-text{
	width:60rpx;
	height:50rpx;
	position:absolute;
	top:50%;
	margin-top:-10rpx;
	line-height:50rpx;
	font-size:26rpx;
	left:30rpx;
}
.clearfix:after {content: "." ;display: block ;height: 0 ;clear:both;visibility:hidden;}
.hover{background:#f0f0f0;}


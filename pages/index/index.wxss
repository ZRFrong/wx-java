/*隐藏滚动条*/

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.comment {
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}

.comment_div {
  width: 100%;
  height: 60%;
  background: #fff;
  position: absolute;
  bottom: -60%;
  overflow: hidden;
  z-index: 11;
  padding-bottom: 150rpx;
}

.comment_top {
  text-align: center;
  font-size: 30rpx;
  line-height: 100rpx;
}

.comment_top text {
  right: 5%;
  font-size: 45rpx;
  position: absolute;
}

.comment .list {
  padding: 0 5%;
  width: 90%;
  overflow: hidden;
  height: calc(100% - 100rpx);
}

.comment .list .list_li {
  padding: 25rpx 0;
}

.comment .list .list_li .pic {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  float: left;
}

.comment .list .list_li .show {
  float: left;
  margin-left: 16rpx;
  width: 460rpx;
}

.comment .list .list_li .show .title {
  font-size: 26rpx;
  color: #6e6e6e;
  margin-bottom: 4rpx;
}

.comment .list .list_li .show .content {
  font-size: 30rpx;
  color: #4c4c4c;
  margin-bottom: 6rpx;
}

.comment .list .list_li .show .time {
  font-size: 26rpx;
  color: #6e6e6e;
}

.comment .list .list_li .up {
  float: right;
}

.comment .list .list_li .up image {
  width: 30rpx;
  height: 30rpx;
  margin-top: 40rpx;
}

.comment .list .list_li .up text {
  font-size: 26rpx;
  margin-left: 5rpx;
  color: #6e6e6e;
}


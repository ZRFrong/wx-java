}
.bg{position:fixed;width:100%;height:100%;background:rgba(0,0,0,0.35);top:0;left:0;z-index:99999;}
.selectdiv{background:#fff;width:480rpx;height:390rpx;position:absolute;left:50%;margin-left:-240rpx;top:50%;margin-top:-195rpx;border-radius:20rpx;}
.selecttop{padding:20rpx 0 12rpx;text-align:center;font-size:30rpx;border-bottom:1rpx solid #dedede;width:90%;margin:0 auto;}
.top_p{font-size:24rpx;color:#b8b8b8;text-align:center;margin-top:6rpx;}
.selectbottom{width:100%;height:200rpx;}
.selectbottom .yiban{width:49%;height:100%;display:inline-block;}
.selectbottom .yiban .yibandiv{border-right:1rpx solid #dedede;margin-top:30rpx;}
.selectbottom .yiban .yibandiv2{margin-top:30rpx;}
.selectbottom .yiban image{width:100rpx;height:100rpx;display:block;margin:0 auto;}
.selectbottom .yibantitle{font-size:26rpx;text-align:center;margin-top:-8rpx;}
.selectbottom .yibantitle checkbox{font-size:44rpx;text-align:center;transform:scale(0.6);}
.select_success{width:100%;height:80rpx;background:#49eebb;color:#fff;text-align:center;border-radius:0 0 20rpx 20rpx;line-height:80rpx;}
.close{width:70rpx;height:70rpx;position:absolute;left:50%;margin-left:-35rpx;top:50%;margin-top:265rpx;}


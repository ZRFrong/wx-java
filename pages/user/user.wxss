.send{position:absolute;background:#57e69b;color:#fff;text-align:center;height:54rpx;top:24rpx;right:30rpx;border-radius:0 45px 45px 0;padding:0 20rpx;z-index:11;line-height:54rpx;font-size:26rpx;box-sizing:inherit;}
